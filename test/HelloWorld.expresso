// Caso de prueba 1: HelloWorld básico del spec
// Cubre: let, print, lambda básica (1 arg), operadores aritméticos básicos

// Lambda simple con un argumento (sin paréntesis)
let square = x -> x * x

// Invocar la lambda y mostrar resultado
print(square(5))

// Operaciones aritméticas básicas
let sum = 10 + 20
print(sum)

let product = 5 * 6
print(product)

// Lambda con suma
let addTen = n -> n + 10
print(addTen(15))

// Expresiones parentizadas
let expr = (2 + 3) * 4
print(expr)
